{
  "name": "running-app",
  "version": "1.0.6",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:dev": "APP_VARIANT=development && npx expo start",
    "test:watch": "jest --watchAll",
    "test": "jest",
    "text:debug": "jest --runInBand --detectOpenHandles --forceExit",
    "build:local:dev": "yarn deps:all && yarn run prebuild && eas build -p android --profile development --local",
    "build:local:preview": "yarn deps:all && yarn prebuild && eas build -p android --profile preview --local",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "eas build -p android --profile preview",
    "build:inspect": "eas build:inspect -p android -s archive -o ../build --profile=preview",
    "lint": "eslint --fix .",
    "deps:all": "yarn && yarn deps:scan && yarn deps:fix",
    "deps:fix": "npx expo install --fix",
    "deps:scan": "npx expo-doctor@latest",
    "deps:check": "npx expo install --check",
    "prebuild": "npx expo prebuild --platform android",
    "prepare": "husky install",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./tests/set-up-async-storage/set-up-async-storage.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@mapbox/polyline": "^1.2.0",
    "@pchmn/expo-material3-theme": "^1.3.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@rnmapbox/maps": "^10.0.14",
    "@supabase/supabase-js": "^2.33.2",
    "@turf/turf": "^6.5.0",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^49.0.11",
    "expo-av": "~13.4.1",
    "expo-dev-client": "~2.4.11",
    "expo-file-system": "~15.4.4",
    "expo-font": "~11.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-linking": "~5.0.2",
    "expo-location": "~16.1.0",
    "expo-router": "2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.16",
    "expo-web-browser": "~12.3.2",
    "geolib": "^3.3.4",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-compressor": "^1.8.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-paper": "^5.10.1",
    "react-native-paper-dates": "^0.18.15",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.2.2",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.47.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "prettier": "^3",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "redux-persist": "^6.0.0",
    "sharp-cli": "^4.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "eslintConfig": {
    "extends": "universe/native",
    "ignorePatterns": [
      "node_modules",
      ".expo",
      "android",
      "assets"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "bracketSameLine": true
  }
}
