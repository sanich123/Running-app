{
  "name": "running-app",
  "version": "2.1.6",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:prod": "expo start --no-dev --minify",
    "test": "cross-env IS_TESTING=true EXPO_PUBLIC_BASE_URL=https://runich-backend-production.up.railway.app jest",
    "test:coverage": "jest --coverage",
    "test:debug": "cross-env IS_TESTING=true jest --runInBand --detectOpenHandles --forceExit",
    "export:analyze:web": "npx expo export --source-maps --platform web",
    "export:analyze:ios": "npx expo export --source-maps --platform ios --no-bytecode",
    "export:analyze:android": "npx expo export --source-maps --platform android --no-bytecode",
    "analyze:web": "source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "build:dev:android:cloud": "eas build --profile development --platform android",
    "build:dev:android:local": "APP_VARIANT=development EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop eas build -p android --profile development --local",
    "build:dev:ios:emulator:cloud": "eas build --profile ios-simulator --platform ios",
    "build:dev:ios:emulator:local": "APP_VARIANT=development EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop eas build --profile ios-simulator --platform ios --local",
    "build:dev:all:local": "yarn run build:dev:android:local && yarn run build:dev:ios:emulator:local",
    "build:preview:android:cloud": "eas build -p android --profile preview",
    "build:preview:android:local": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop eas build -p android --profile preview --local",
    "build:preview:ios:emulator": "eas build -p ios --profile preview",
    "build:preview:ios:emulator:local": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop eas build -p ios --profile preview --local",
    "build:preview:all:local": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop bun run build:preview:android:local && bun run build:preview:ios:emulator:local",
    "build:all:local": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=~/Desktop bun run build:preview:android:local && bun run build:preview:ios:emulator:local && bun run build:dev:android:local && bun run build:dev:ios:emulator:local",
    "build:inspect": "eas build:inspect -p android -s archive -o ../build --profile=preview",
    "lint": "yarn expo lint",
    "deps:all": "yarn && yarn run deps:scan && yarn run deps:fix",
    "deps:fix": "npx expo install --fix",
    "deps:scan": "npx expo-doctor@latest",
    "deps:check": "npx expo install --check",
    "prebuild:android": "npx expo prebuild --clean --platform android",
    "prebuild:ios": "npx expo prebuild --clean --platform ios",
    "prebuild:all": "npx expo prebuild --clean",
    "prepare": "husky install",
    "android": "expo run:android --variant debug",
    "android:preview": "expo run:android --variant release",
    "ios": "expo run:ios",
    "web:export": "expo export -p web",
    "web:serve:single": "npx serve dist --single",
    "web:serve": "npx serve dist",
    "web:deploy:netlify": "netlify deploy --dir dist",
    "web:deploy:prod": "netlify deploy --prod",
    "web:deploy:full": "yarn web:export && netlify deploy --prod"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest-setup.ts"
    ],
    "setupFiles": [
      "./tests/set-up-async-storage/set-up-async-storage.ts",
      "./node_modules/@react-native-google-signin/google-signin/jest/build/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase|@rnmapbox)"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^4",
    "@mapbox/polyline": "^1.2.0",
    "@pchmn/expo-material3-theme": "^1.3.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@rnmapbox/maps": "^10.1.28",
    "@supabase/supabase-js": "^2.45.1",
    "@turf/bbox": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "aes-js": "^3.1.2",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~51.0.31",
    "expo-auth-session": "~5.5.2",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-camera": "~15.0.15",
    "expo-dev-client": "~4.0.25",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.9",
    "expo-image": "~1.12.15",
    "expo-image-picker": "~15.0.7",
    "expo-intent-launcher": "~11.0.1",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-media-library": "~16.0.4",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-task-manager": "~11.8.2",
    "expo-updates": "~0.25.24",
    "expo-video-thumbnails": "~8.0.0",
    "expo-web-browser": "~13.0.3",
    "mapbox-gl": "^1.13.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-battery-optimization-check": "^1.0.8",
    "react-native-compressor": "^1.8.3",
    "react-native-date-picker": "^4.3.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-charts": "^1.4.30",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-pager-view": "6.3.0",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.1.3",
    "turf-linestring": "^1.0.2",
    "turf-point": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.2",
    "@types/aes-js": "^3.1.4",
    "@types/jest": "^29.5.3",
    "@types/mapbox-gl": "^2.7.20",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "abort-controller": "^3.0.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "msw": "^1.1.0",
    "prettier": "^3.3.2",
    "react-native-zip-archive": "^7.0.1",
    "react-test-renderer": "18.2.0",
    "redux-persist": "^6.0.0",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "ignorePatterns": [
      "node_modules",
      ".expo",
      "android",
      "assets",
      "ios",
      ".husky",
      "assets",
      "coverage",
      ".yarn",
      "dist"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "bracketSameLine": true
  }
}
