{
  "name": "running-app",
  "version": "2.0.0",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:dev": "APP_VARIANT=development && npx expo start",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test": "set IS_TESTING=true&&jest",
    "test:windows": "set IS_TESTING=true&&jest",
    "test:debug": "set IS_TESTING=true&&jest --runInBand --detectOpenHandles --forceExit",
    "build:dev:all:local": "bun run build:dev:android:local && bun run build:dev:ios:emulator:local",
    "build:preview:all:local": "bun run build:preview:android:local && bun run build:preview:ios:emulator:local",
    "build:all:local": "bun run build:android:all && bun run build:ios:all",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:android:local": "APP_VARIANT=development eas build -p android --profile development --local",
    "build:preview:android": "eas build -p android --profile preview",
    "build:preview:android:local": "eas build -p android --profile preview --local",
    "build:android:all": "bun run build:dev:android:local && bun run build:preview:android:local",
    "build:dev:ios:emulator": "eas build --profile ios-simulator --platform ios",
    "build:dev:ios:emulator:local": "APP_VARIANT=development eas build --profile ios-simulator --platform ios --local",
    "build:preview:ios:emulator": "eas build -p ios --profile preview",
    "build:preview:ios:emulator:local": "eas build -p ios --profile preview --local",
    "build:ios:all": "bun run build:preview:ios:emulator:local && bun run build:dev:ios:emulator:local",
    "build:inspect": "eas build:inspect -p android -s archive -o ../build --profile=preview",
    "lint": "eslint --fix .",
    "check:ts": "tsc",
    "check:all": "yarn lint && yarn check:ts && yarn test",
    "deps:all": "yarn && bun run deps:scan && bun run deps:fix",
    "deps:fix": "npx expo install --fix",
    "deps:scan": "npx expo-doctor@latest",
    "deps:check": "npx expo install --check",
    "prebuild:android": "npx expo prebuild --clean --platform android",
    "prebuild:ios": "npx expo prebuild --clean --platform ios",
    "prebuild:all": "npx expo prebuild --clean",
    "prepare": "husky install",
    "android": "expo run:android --variant debug",
    "android:preview": "expo run:android --variant release",
    "ios": "expo run:ios",
    "migrate:bun": "npx synp --source-file yarn.lock && npm i --lockfile-version 3 --frozen-lockfile && bun pm migrate"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest-setup.ts"
    ],
    "setupFiles": [
      "./tests/set-up-async-storage/set-up-async-storage.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase|@rnmapbox)"
    ],
    "moduleNameMapper": {
      "^@c/(.*)": "<rootDir>/components/$1",
      "^@u/(.*)": "<rootDir>/utils/$1",
      "^@app/(.*)": "<rootDir>/app/$1",
      "^@auth/(.*)": "<rootDir>/auth/$1",
      "^@const/(.*)": "<rootDir>/constants/$1",
      "^@r/(.*)": "<rootDir>/redux/$1"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@mapbox/polyline": "^1.2.0",
    "@pchmn/expo-material3-theme": "^1.3.1",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@rnmapbox/maps": "^10.1.9",
    "@shopify/flash-list": "1.6.3",
    "@supabase/supabase-js": "^2.33.2",
    "@turf/bbox": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~50.0.5",
    "expo-camera": "~14.0.3",
    "expo-dev-client": "~3.3.8",
    "expo-file-system": "~16.0.5",
    "expo-font": "~11.10.2",
    "expo-image-picker": "~14.7.1",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.2",
    "expo-media-library": "~15.9.1",
    "expo-router": "~3.4.6",
    "expo-secure-store": "~12.8.1",
    "expo-sharing": "~11.10.0",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-task-manager": "~11.7.2",
    "expo-updates": "~0.24.9",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-compressor": "^1.8.3",
    "react-native-date-picker": "^4.3.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-pager-view": "6.2.3",
    "react-native-paper": "^5.10.1",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.0.7",
    "sharp-cli": "^2.1.0",
    "turf-linestring": "^1.0.2",
    "turf-point": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.14",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.0.0",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.47.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
    "msw": "^1.1.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "redux-persist": "^6.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "eslintConfig": {
    "extends": "universe/native",
    "ignorePatterns": [
      "node_modules",
      ".expo",
      "android",
      "assets",
      "ios",
      ".husky",
      "assets",
      "coverage",
      ".yarn"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "bracketSameLine": true
  }
}
