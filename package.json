{
  "name": "running-app",
  "version": "1.1.11",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:dev": "APP_VARIANT=development && npx expo start",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test": "IS_TESTING=true jest",
    "test:windows": "set IS_TESTING=true&&jest",
    "test:debug": "set IS_TESTING=true&&jest --runInBand --detectOpenHandles --forceExit",
    "build:local:dev:android": "eas build -p android --profile development --local",
    "build:local:dev:ios": "eas build -p ios --profile development --local",
    "build:local:preview:android": "eas build -p android --profile preview --local",
    "build:local:preview:ios": "eas build -p ios --profile preview --local",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview:android": "eas build -p android --profile preview",
    "build:preview:ios": "eas build -p ios --profile preview",
    "build:inspect": "eas build:inspect -p android -s archive -o ../build --profile=preview",
    "lint": "eslint --fix .",
    "check:ts": "tsc",
    "check:all": "yarn lint && yarn check:ts && yarn test",
    "deps:all": "yarn && bun run deps:scan && bun run deps:fix",
    "deps:fix": "npx expo install --fix",
    "deps:scan": "npx expo-doctor@latest",
    "deps:check": "npx expo install --check",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild:all": "npx expo prebuild",
    "prepare": "husky install",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "migrate:bun": "npx synp --source-file yarn.lock && npm i --lockfile-version 3 --frozen-lockfile && bun pm migrate"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest-setup.ts"
    ],
    "setupFiles": [
      "./tests/set-up-async-storage/set-up-async-storage.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@mapbox/polyline": "^1.2.0",
    "@pchmn/expo-material3-theme": "^1.3.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@rnmapbox/maps": "^10.1.1",
    "@supabase/supabase-js": "^2.33.2",
    "@turf/bbox": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^49.0.11",
    "expo-camera": "~13.4.4",
    "expo-dev-client": "~2.4.12",
    "expo-file-system": "~15.4.5",
    "expo-font": "~11.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-linking": "~5.0.2",
    "expo-location": "~16.1.0",
    "expo-media-library": "~15.4.1",
    "expo-router": "2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-sharing": "~11.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.18",
    "expo-web-browser": "~12.3.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-compressor": "^1.8.3",
    "react-native-date-picker": "^4.3.3",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-paper": "^5.10.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-view-shot": "3.7.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.0.7",
    "turf-linestring": "^1.0.2",
    "turf-point": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "msw": "^1.1.0",
    "prettier": "^3",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "redux-persist": "^6.0.0",
    "sharp-cli": "^4.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "eslintConfig": {
    "extends": "universe/native",
    "ignorePatterns": [
      "node_modules",
      ".expo",
      "android",
      "assets"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "bracketSameLine": true
  },
  "packageManager": "yarn@4.0.2"
}
