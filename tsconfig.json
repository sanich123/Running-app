{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@c/*": [
        "components/*"
      ],
      "@app/*": [
        "app/*"
      ],
      "@auth/*": [
        "auth/*"
      ],
      "@const/*": [
        "constants/*"
      ],
      "@r/*": [
        "redux/*"
      ],
      "@u/*": [
        "utils/*"
      ]
    }
  },
  "lib": [
    "dom",
    "esnext",
    "dom.iterable"
  ],
  "jsx": "react",
  "module": "esnext",
  "moduleResolution": "node",
  "noImplicitAny": true,
  "noFallthroughCasesInSwitch": true,
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
